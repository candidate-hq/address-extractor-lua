    function closeTree() {
        height = $('#jstree_demo_div').height();
        heightTranslate = height / 2 - 22;
        $('#jstree_demo_div').css('height', height + 'px');
        $('.hideTreeIcon').css('transform', 'rotate(180deg)').css('transition-duration', '0.5s');
        //$('.hideTreeIcon').css('transform', 'translate(0,'+''+heightTranslate+'px'+') rotate(180deg)').css('transition-duration','0.5s');
        //$('#jstree_demo_div').css('background','transparent');
        $('#jstree_demo_div').css('max-width', '2%').css('transition-duration', '0.5s');
        $('.tvd').css('opacity', '0').css('transition-duration', '0.5s');
        $('#jstree_demo_div ul').css('opacity', '0').css('transition-duration', '0.5s');
        $(this).removeClass('hideTreeIcon').addClass('showTreeIcon');
        $('#jstree_demo_div').addClass('swapped');
        localStorage.setItem("treeState", "true");
        //$('.tree').hide();
        //$('.treeIcon').show();
    }

    function openTree() {
        $('#jstree_demo_div').removeClass('swapped')
        //$('.tree').show();
        //$('.treeIcon').hide();
        //$('.showTreeIcon').css('transform', 'translate(0px, 0px) rotate(1deg)').css('transition-duration','0.5s');
        $('.showTreeIcon').css('transform', 'rotate(1deg)').css('transition-duration', '0.5s');
        $('#jstree_demo_div').css('max-width', '100%').css('height', '100%').css('transition-duration', '0.5s');
        ;
        $('#jstree_demo_div ul').css('opacity', '1').css('transition-duration', '0.5s');
        $('.tvd').css('opacity', '1').css('transition-duration', '0.5s');
        $('.showTreeIcon').removeClass('showTreeIcon').addClass('hideTreeIcon');
        localStorage.setItem("treeState", "false");
    }


    if (tvdTreeJson != "") {
        var mainul = createUl($('#jstree_demo_div'));
        var mainli = createLi(mainul, tvdTreeJson);
        iterateTree(tvdTreeJson, mainli);


        $(document).ready(function () {
            $(document).on('click', '.hideTreeIcon', closeTree);
            $(document).on('click', '.swapped', openTree);

            $(document).on('click', '.tree-open', function () {
                //$(this).next().next().next().css("display", "none");
                //$(this).closest('ul').css("display", "none");
                //$(this).next().next('ul').css("display", "none");
                $(this).parent().find('ul').css("display", "none");
                $(this).removeClass('tree-open').addClass('tree-close');
                $(this).html(arrOpen);
            });

            if ($('#0').length > 0 && selectedPrver === "1") {
                $('#0').children(":first").removeClass('tree-open').addClass('tree-close');
                $('#0').children(":first").html(arrOpen);
                $('#0').children('ul').css("display", "none");
            }

            $(document).on('click', '.need-load', function () {
                $(this).removeClass('need-load');
                var tvd = $(this).parent().attr('id');
                var url = "/region/izbirkom?action=tvdTree&tvdchildren=true&vrn=" + vrnvibref + "&tvd=" + tvd;
                var parent = $(this).parent();
                $.ajax({
                    type: "GET",
                    url: url,
                    beforeSend: function () {
                        $(".loader-template").show();
                    },
                    success: function (data) {
                        addChildren(data, parent);
                        $(".loader-template").hide();
                    }
                });

                //
                // $.get(url, function (data) {
                // 	addChildren(data, parent);
                // })
                // $(this).next().next().css("display", "none");
                // $(this).removeClass('tree-open').addClass('tree-close');
                // $(this).html("&#9658;");
            });


            $(document).on('click', '.tree-close', function () {
                //$(this).next().next().next().css("display", "block");
                //$(this).closest('ul').css("display", "block");
                $(this).parent().find('ul').css("display", "block");
                //$(this).next().next('ul').css("display", "block");
                $(this).removeClass('tree-close').addClass('tree-open');
                $(this).html(arrClose);
            });

            // $(".tree-close").click(function() {

            // });
        });
    }


    
