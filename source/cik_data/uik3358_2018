    function setRedirectFlag() {
        localStorage.setItem("doOnce", "true");
        localStorage.setItem("redirectFromElection", "true");
    }

    $(document).ready(function () {
        var doOnce = localStorage.getItem("doOnce");
        var redirect = localStorage.getItem("redirectFromElection");
        if (redirect === "true" && doOnce === "true") {
            localStorage.setItem("doOnce", "false");
            $('#search_form').submit();
        }
    });

