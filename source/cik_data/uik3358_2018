    $(document).ready(function () {
        $("#election-info-name-mobile,#standard-reports-name-mobile,#election-process-name-mobile,#digital-election-sites-name-mobile,#digital-election-vote-name-mobile,#digital-remote-election-data-name-mobile,#information-about-candidates-name-mobile,#payment-results-name-mobile,#election-results-name-mobile,#vote-results-name-mobile,#report-body-link-mobile").on('click', function () {
            $("#mobile__menu__nav").hide();
        })

        $(".mobile-menu-button").on('click', function () {
            $("#mobile__menu__nav").show();
            // if ($("#election-info-name-mobile").text() != '') {
            // 	$("#mobile__menu__nav").css('height','100%');
            // } else $("#mobile__menu__nav").css('min-height','100%');
        })

        $(".close-mobile-menu").on('click', function () {
            $("#mobile__menu__nav").hide();
        })

        $(".loader-template").hide();
        $('html, body').css({overflow: 'auto', height: 'auto'});

        $('.tree-li').on('click', 'a', function () {
            if ($(this).hasClass('tree-open') || $(this).hasClass('tree-close')) {
                return;
            }
            $(".loader-template").show();
            $('html, body').css({overflow: 'hidden', height: '100%'});
        })


        $(".tdReport a,.viboryLink,.searchCandByParty,.list-link,#calendar-btn-search,.breadcrumb a,#calendar-btn,.pagination-new a,.calYears,.calMonths,.oldDay,.newDay").on('click', function (e) {

            if ($(this).parent().attr('class') == 'tree-li' && $("#back-to-list-btn").length != 0) {
                e.preventDefault();
                window.location = localStorage.getItem("back-to-list-link");
            } else if ($(this).attr('id') == 'calendar-btn-search') {
                sessionStorage.setItem("isNewSession", "false")
                $(".loader-template").show();
                $('html, body').css({overflow: 'hidden', height: '100%'});
            } else if ($(this).attr('class') == 'calYears' || $(this).attr('class') == 'calMonths' || $(this).attr('class') == 'oldDay' || $(this).attr('class') == 'newDay') {
                $(".loader-template").show();
                $('html, body').css({overflow: 'hidden', height: '100%'});
            } else if ($(this).attr('id') == 'finRep' || $(this).attr('href').match('javascript')) {
                return;
            } else {
                $(".loader-template").show();
                $('html, body').css({overflow: 'hidden', height: '100%'});
            }
        })
    })

    

    


