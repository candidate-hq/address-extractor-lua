    $(document).ready(function () {
        var directLink = localStorage.getItem("link-from-list")
        if (directLink == "false") {
            $("#back-to-list-btn").hide();
        } else if (directLink == "true") {
            $("#back-to-list-btn").show();
        }
    })

    $(document).ready(function () {
        var boolean = localStorage.getItem("back-to-candidates-show")
        if (boolean == "false") {
            $("#back-to-candidates-btn").hide();
        } else if (boolean == "true") {
            $("#back-to-candidates-btn").show();
        }
    })

    $(document).on('click', '#calendar-table .btn-block', function () {
        var startDate = $(this).attr('onclick').substring(15, 25);
        var endDate = $(this).attr('onclick').substring(29, 39);
        localStorage.setItem("start_date", startDate);
        localStorage.setItem("end_date", endDate);
        $("#urovproved, #vidvibref, #vibtype, #actual_regions_subjcode, #old_regions_subjcode, #sxemavib")
            .multiselect('deselectAll', false);
        localStorage.setItem("urovproved", "");
        localStorage.setItem("vidvibref", "");
        localStorage.setItem("vibtype", "");
        localStorage.setItem("actual_regions_subjcode", "");
        localStorage.setItem("old_regions_subjcode", "");
        localStorage.setItem("sxemavib", "");
        $("#start_date").val(startDate);
        $("#end_date").val(endDate);
        $("#search_form").submit();
    })

    $(document).on('click', '#calendar-btn', function () {
        localStorage.setItem("start_date", getStartDate());
        localStorage.setItem("end_date", getEndDate());
        $("#urovproved, #vidvibref, #vibtype, #actual_regions_subjcode, #old_regions_subjcode, #sxemavib")
            .multiselect('deselectAll', false);
        localStorage.setItem("urovproved", "");
        localStorage.setItem("vidvibref", "");
        localStorage.setItem("vibtype", "");
        localStorage.setItem("actual_regions_subjcode", "");
        localStorage.setItem("old_regions_subjcode", "");
        localStorage.setItem("sxemavib", "");
    })

    function getStartDate() {
        let dateTime = luxon.DateTime;
        return dateTime.local().minus({months: 1}).startOf('month').toFormat("dd.MM.yyyy");
    }

    function getEndDate() {
        let dateTime = luxon.DateTime;
        return dateTime.local().plus({months: 4}).endOf('month').toFormat("dd.MM.yyyy");
    }

