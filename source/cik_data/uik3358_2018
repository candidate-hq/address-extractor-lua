        $(document).ready(function () {
            let isInetCopy = 'true';
            if (isInetCopy === 'true') {
                $(".link-to-summery-table").remove();
            }
        });

        fpactionxyz('http://www.moscow-city.vybory.izbirkom.ru/');
        $(document).ready(function () {
            $(document.body).prepend('<a href="#" class="bvi-open" title="Версия сайта для слабовидящих" style="float: right;margin-right: 25px;">Версия сайта для слабовидящих</a>');
            $.bvi({
            	'bvi_target' : '.bvi-open', // Класс ссылки включения плагина
            	"bvi_theme" : "black", // Цвет сайта
            	"bvi_font" : "arial", // Шрифт
            	"bvi_font_size" : 16, // Размер шрифта
            	"bvi_letter_spacing" : "normal", // Межбуквенный интервал
            	"bvi_line_height" : "normal", // Междустрочный интервал
            	"bvi_images" : true, // Изображения
            	"bvi_reload" : true, // Перезагрузка страницы при выключении плагина
            	"bvi_fixed" : false, // Фиксирование панели для слабовидящих вверху страницы
            	"bvi_tts" : false, // Синтез речи
            	"bvi_flash_iframe" : true, // Встроенные элементы (видео, карты и тд.)
            	"bvi_hide" : false // Скрывает панель для слабовидящих и показывает иконку панели.
            });

            localStorage.setItem("filtered-list", "/region/izbirkom");
            if ($('.page-link').length > 1) {
                var pageNum = window.location.href.match(/number=(\d+)/)
                var link = $('.page-link').attr('href') == undefined ? $('.page-link').eq($('.page-link').length - 1).attr('href') : $('.page-link').attr('href')
                if (localStorage.getItem('page') != null && pageNum != null) {
                    if (pageNum[1] == localStorage.getItem('page')) {
                        $('.pagination-new').pagination({
                            items: $('.page-item').length,
                            prevText: '',
                            currentPage: localStorage.getItem('page'),
                            nextText: '',
                            displayedPages: 15,
                            hrefTextPrefix: link.replace(/\d+$/, ''),
                            cssStyle: 'light-theme'
                        });
                    } else {
                        $('.pagination-new').pagination({
                            items: $('.page-item').length,
                            prevText: '',
                            nextText: '',
                            currentPage: pageNum[1],
                            displayedPages: 15,
                            hrefTextPrefix: link.replace(/\d+$/, ''),
                            cssStyle: 'light-theme'
                        });
                    }
                } else {
                    $('.pagination-new').pagination({
                        items: $('.page-item').length,
                        prevText: '',
                        nextText: '',
                        displayedPages: 15,
                        hrefTextPrefix: link.replace(/\d+$/, ''),
                        cssStyle: 'light-theme'
                    });
                }
                $('.pagination-new a').click(function () {
                    localStorage.setItem('page', $(this).text())
                });
            } //else localStorage.removeItem('page');

            $.fn.dataTable.moment("DD.MM.YYYY");

            var p = $('#politparty,#politparty2').DataTable({
                paging: false,
                searching: false,
                info: false,
                order: [1, 'asc'],
                //orderClasses: true,
                language: {
                    emptyTable: "Нет данных для построения отчета"
                },
                orderCellsTop: true,
            });

            var t = $('#candidates-220-2,#candidates-221-1,#candidates-221-2').DataTable({
                paging: false,
                searching: false,
                info: false,
                order: [0, 'asc', 1, 'asc'],
                //orderClasses: true,
                language: {
                    emptyTable: "Нет данных для построения отчета"
                },
                columnDefs: [{
                    "targets": 0,
                    "orderable": false,
                    "searchable": false
                }],
                orderCellsTop: true,
            });

            var z = $('#candidates-220-1').DataTable({
                paging: false,
                searching: false,
                info: false,
                order: [[3, 'asc'], [5, 'asc']],
                //orderClasses: true,
                language: {
                    emptyTable: "Нет данных для построения отчета"
                },
                columnDefs: [{
                    "targets": 0,
                    "orderable": false,
                    "searchable": false
                }],
                orderCellsTop: true,
            });


            var b = parseInt(z.cells().data()[0])

            var pageNumber = parseInt($('li.active > span.current').text())

            if (z.column(0, {page: 'current'}).nodes() != undefined) {
                z.column(0, {page: 'current'}).nodes().each(function (cell, i) {
                    isNaN(pageNumber) ? cell.innerHTML = i + 1 : cell.innerHTML = (i + 1) + ((pageNumber - 1) * 20);
                    //	b != 1 && $('.page-link').length > 1 ? cell.innerHTML = i + b : cell.innerHTML = i + 1
                });

                z.on('order', function () {
                    z.column(0, {page: 'current'}).nodes().each(function (cell, i) {
                        isNaN(pageNumber) ? cell.innerHTML = i + 1 : cell.innerHTML = (i + 1) + ((pageNumber - 1) * 20);
                    });
                })

            }

            if (p.column(0, {page: 'current'}).nodes() != undefined) {
                p.column(0, {page: 'current'}).nodes().each(function (cell, i) {
                    isNaN(pageNumber) ? cell.innerHTML = i + 1 : cell.innerHTML = (i + 1) + ((pageNumber - 1) * 20);
                    //	b != 1 && $('.page-link').length > 1 ? cell.innerHTML = i + b : cell.innerHTML = i + 1
                });

                p.on('order', function () {
                    p.column(0, {page: 'current'}).nodes().each(function (cell, i) {
                        isNaN(pageNumber) ? cell.innerHTML = i + 1 : cell.innerHTML = (i + 1) + ((pageNumber - 1) * 20);
                    });
                })

            }

            var a = parseInt(t.cells().data()[0])

            if (t.column(0, {page: 'current'}).nodes() != undefined) {
                t.column(0, {page: 'current'}).nodes().each(function (cell, i) {
                    isNaN(pageNumber) ? cell.innerHTML = i + 1 : cell.innerHTML = (i + 1) + ((pageNumber - 1) * 20);
                });

                t.on('order', function () {
                    t.column(0, {page: 'current'}).nodes().each(function (cell, i) {
                        isNaN(pageNumber) ? cell.innerHTML = i + 1 : cell.innerHTML = (i + 1) + ((pageNumber - 1) * 20);
                    });
                })
            }


            $('.dataTables_length').addClass('bs-select');

            if (localStorage.getItem("treeState") == 'true') {
                closeTree();
            } else openTree();

        });

        $(document).ready(function () {
            $(".nav-link").click(function () {
                localStorage.removeItem('page');
                $(".nav-link.active.show").parent().css("background", "none");
                $(this).parent().css("background", "rgba(255, 255, 255, 0.2)");
                $(".rep-name").text("");
                $(".breadcrumb-repname").text("");
            });
            $(".nav-link.active.show").parent().css("background", "rgba(255, 255, 255, 0.2)");
            $("#report-body-link").parent().css("background", "none");
        });

        $(document).ready(function () {
            let name = $("#rep-name").text();
            let namio220 = $(".namio220").text().trim();
            if (namio220 != "") {
                $(".breadcrumb").append("<li class='breadcrumb-repname'>&ensp;&frasl;&ensp;" + name + namio220 + "</li>");
            } else $(".breadcrumb").append("<li class='breadcrumb-repname'>&ensp;&frasl;&ensp;" + name + "</li>");
        });

        function hideSidebar() {

            if ($("#sidebar").css("left") == "-338px" && $(".page-wrapper").hasClass("toggled")) {
                $(".page-wrapper").toggleClass("toggled");
                $("#show-sidebar,.close-m").click(function () {
                    $("#sidebar").css('left', 0);
                });
                $("#close-sidebar,.close-m").click(function () {
                    $("#sidebar").css('left', -338);
                })
            }
        }

        $(document).ready(function () {
            let link = localStorage.getItem("election-link");
            if (link === "" || link == null) {
                $(".election-card").hide();
            }
        });
        $(document).ready(function () {
            hideSidebar();
            $("#close-sidebar").click(function () {
                $(".page-wrapper").toggleClass("toggled");
            });
            $("#show-sidebar").click(function () {
                $(".page-wrapper").toggleClass("toggled");
            });
        });
        $(document).ready(function () {
            let link = localStorage.getItem("election-link");
            $(".election-card").attr("href", link);
        });
