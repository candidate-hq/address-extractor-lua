<script>


    function isElementInViewport(el) {
        var rect = el.getBoundingClientRect();

        return rect.bottom > 0 && rect.top < (window.innerHeight || document.documentElement.clientHeight) /* or $(window).height() */;
    }

    function showVib() {


        let elems = document.querySelectorAll('.vibory-list-item')
        Array.prototype.forEach.call(elems, function (node) {
            if (!isElementInViewport(node)) return
            var currentNode = node.nextElementSibling;
            var cnt = 0;
            while (!currentNode.classList.contains('vibory-list-item') && cnt <= 50) {
                currentNode.classList.remove('hidden-vibory');
                currentNode = currentNode.nextElementSibling;
                cnt++;
            }
        })
        elems = document.querySelectorAll('.list-group-item')
        Array.prototype.forEach.call(elems, function (node) {
            if (!isElementInViewport(node)) return
            var currentNode = node;
            var cnt = 0;

            while (cnt <= 50) {
                if (currentNode != null) {
                    currentNode.classList.remove('hidden-vibory');
                    currentNode = currentNode.previousSibling;

                }
                cnt++;
            }
            currentNode = node;
            cnt = 0;

            while (cnt <= 50) {
                if (currentNode != null) {
                    currentNode.classList.remove('hidden-vibory');
                    currentNode = currentNode.nextElementSibling;
                }
                cnt++;
            }
        })
    }

    window.addEventListener('scroll', showVib, {capture: true});
</script>
</body>
</html>
