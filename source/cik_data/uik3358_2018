        function iterateTree(node, li) {
            if (node.children.length > 0) {
                var ul = createUl(li);
                node.children.forEach(el => {
                    var liChild = createLi(ul, el);
                    iterateTree(el, liChild);
                })
            }
        }

    function addChildren(data, li) {
        var ul = createUl(li);
        data.forEach(el => {
            //var liChild = createLi(ul, el);
            createLi(ul, el);
            //iterateTree(el,liChild);
        })
    }

    function createUl(element) {
        var ul = $('<ul></ul>');
        ul.appendTo(element);
        return ul;
        //return element.append('<ul></ul>').children("ul:first-child");
    }

