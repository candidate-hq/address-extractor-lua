    var reportModeUrl = report_mode !== "" ? "&report_mode=" + report_mode : "";
    var selectedPrver;

    function createLi(element, obj) {
        let button = "";
        let need_load = obj.load_on_demand ? "need-load" : "";
        if (!obj.isUik) {
            if (obj.children.length > 0) {
                button = "<a class='tree-open " + need_load + "' >" + arrClose + "</a>";
            } else {
                button = "<a class='tree-close " + need_load + "'>" + arrOpen + "</a>";
            }
        }
        if (obj.selected) {
            selectedPrver = obj.prver;
        }
        element.append('<li id="' + obj.id + '" class="tree-li">' + button + "<a  class='" + (obj.selected ? "selected-li" : "") + "' href='" + obj.href + "&type=" + vid + vibidUrl + reportModeUrl + "'> " + obj.text + "</a> " + (obj.selected ? opened : "") + '</li>');
        //document.getElementById(obj.id).appendChild(buttonNode);
        //$('#'+obj.id).append(button);
        return $('#' + obj.id);
    }


